{
    "configurations": [
        {
            "name": "Linux-GCC",
            "includePath": [
                // 项目路径
                "${workspaceFolder}/**",
                "${workspaceFolder}/include",
                "${workspaceFolder}/src/include",
                "${workspaceFolder}/third_party/include",
                
                // 系统标准库路径
                "/usr/include",
                "/usr/local/include",
                "/usr/include/x86_64-linux-gnu",
                "/usr/lib/gcc/x86_64-linux-gnu/*/include",
                
                // QT完整头文件路径（系统安装版）
                "/usr/include/x86_64-linux-gnu/qt5",
                "/usr/include/x86_64-linux-gnu/qt5/QtCore",
                "/usr/include/x86_64-linux-gnu/qt5/QtGui",
                "/usr/include/x86_64-linux-gnu/qt5/QtWidgets",
                "/usr/include/x86_64-linux-gnu/qt5/QtNetwork",
                "/usr/include/x86_64-linux-gnu/qt5/QtSql",
                "/usr/include/x86_64-linux-gnu/qt5/QtXml",
                "/usr/include/x86_64-linux-gnu/qt5/QtMultimedia",
                "/usr/include/x86_64-linux-gnu/qt5/QtOpenGL",
                "/usr/include/x86_64-linux-gnu/qt5/QtQml",
                "/usr/include/x86_64-linux-gnu/qt5/QtQuick",
                
                // QT手动安装版路径（可选）
                "/opt/Qt/6.5.0/gcc_64/include/**",
                
                // 常见第三方库路径
                "/usr/include/eigen3",
                "/usr/include/opencv4",
                "/usr/local/boost/include",
                "/usr/include/SDL2",
                "/usr/include/jsoncpp",
                "/usr/include/spdlog"
            ],
            "defines": [
                "_DEBUG",
                "LINUX",
                "__GNUC__",
                "QT_CORE_LIB",
                "QT_GUI_LIB",
                "QT_WIDGETS_LIB",
                "QT_NETWORK_LIB",
                "QT_SQL_LIB",
                "QT_NO_DEBUG",
                "PROJECT_VERSION=\"1.0.0\"",
                "COMPILER_GCC"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "gcc-x64",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "configurationProvider": "ms-vscode.cmake-tools",
            "browse": {
                "path": [
                    "${workspaceFolder}/**",
                    "/usr/include",
                    "/usr/local/include",
                    "/usr/include/x86_64-linux-gnu/qt5/**"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
            }
        },
        {
            "name": "Linux-Clang",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/include",
                "/usr/include",
                "/usr/local/include",
                "/usr/include/x86_64-linux-gnu",
                "/usr/lib/clang/*/include",
                "/usr/include/x86_64-linux-gnu/qt5/**"
            ],
            "defines": [
                "_DEBUG",
                "LINUX",
                "__clang__",
                "QT_CORE_LIB",
                "QT_GUI_LIB",
                "QT_WIDGETS_LIB",
                "COMPILER_CLANG"
            ],
            "compilerPath": "/usr/bin/clang++",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "clang-x64",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json"
        },
        {
            "name": "Linux-Debug",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/include",
                "/usr/include",
                "/usr/include/x86_64-linux-gnu/qt5/**"
            ],
            "defines": [
                "_DEBUG",
                "DEBUG",
                "LINUX",
                "QT_CORE_LIB",
                "QT_GUI_LIB",
                "QT_WIDGETS_LIB",
                "QT_DEBUG"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        },
        {
            "name": "Linux-Release",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/include",
                "/usr/include",
                "/usr/include/x86_64-linux-gnu/qt5/**"
            ],
            "defines": [
                "NDEBUG",
                "RELEASE",
                "LINUX",
                "QT_CORE_LIB",
                "QT_GUI_LIB",
                "QT_WIDGETS_LIB",
                "QT_NO_DEBUG"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4,
    "env": {
        "myDefaultIncludePath": [
            "${workspaceFolder}",
            "${workspaceFolder}/include"
        ],
        "myCompilerPath": "/usr/bin/g++"
    }
}